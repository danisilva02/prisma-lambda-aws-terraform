{
  "name": "my-company-api",
  "version": "1.0.0",
  "main": "src/lambda.ts",
  "license": "MIT",
  "scripts": {
    "start": "yarn build && sleep 5 && yarn dist",
    "dist": "node infrastructure/dist/main.js",
    "build": "NODE_ENV=production run-s clean build:lambda",
    "build:lambda": "node ./esbuild.js && yarn setup:lib",
    "setup:lib": "gulp copy:modules",
    "clean": "rimraf ./infrastructure/dist && rimraf ./infrastructure/lambda  && rimraf ./infrastructure/lambda.zip",
    "lint": "eslint",
    "lint:all": "eslint . --ext .js,.ts",
    "postinstall": "is-ci || husky install",
    "prettier": "prettier",
    "test": "jest ./test",
    "prisma:generate": "npx prisma generate --schema ./prisma/schema.prisma",
    "prisma:generate:prod": "PRISMA_BINARY_TARGET=rhel-openssl-1.0.x prisma generate",
    "prisma:migrate:create": "npx prisma migrate dev --preview-feature",
    "prisma:migrate:reset": "npx prisma migrate reset --preview-feature",
    "prisma:migrate": "npx prisma db push",
    "prisma:studio": "npx prisma studio",
    "prisma:format": "npx prisma format"
  },
  "dependencies": {
    "@prisma/client": "3.9.1",
    "aws-lambda": "^1.0.6",
    "aws-lambda-fastify": "^2.1.0",
    "aws-serverless-express": "^3.3.5",
    "dotenv": "^16.0.0",
    "fastify": "^3.27.0",
    "fastify-plugin": "^3.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@types/aws-lambda": "^8.10.83",
    "@types/dotenv-flow": "^3.2.0",
    "@types/jest": "^27.0.2",
    "@types/node": "ts4.4",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "esbuild": "^0.13.4",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.6.0",
    "eslint-plugin-prettier": "^4.0.0",
    "gulp": "^3.9.1",
    "gulp-less": "^3.1.0",
    "gulp-npm-dist": "^0.1.2",
    "gulp-rename": "^1.2.2",
    "husky": "^7.0.2",
    "is-ci": "^3.0.0",
    "jest": "^27.2.5",
    "lint-staged": "^11.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "serverless-dotenv-plugin": "3.12.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3"
  },
  "resolutions": {
    "graceful-fs": "^4.2.4"
  }
}
